{
    "root": true,
    "extends": [
      "next",
      "next/core-web-vitals",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"],
      "react/jsx-curly-brace-presence": ["error", { "props": "never", "children": "never" }],
      "react/self-closing-comp": "error",
      "@next/next/no-html-link-for-pages": "off"
    }
  }
  